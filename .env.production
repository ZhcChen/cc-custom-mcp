# 正式环境配置
MCP_DEV_MODE=false
MCP_LOG_LEVEL=info
MCP_PORT=3000
MCP_HOST=localhost

# 正式版禁用调试工具
MCP_ENABLE_DEBUG_TOOLS=false
MCP_ENABLE_HOT_RELOAD=false

# 正式版数据路径
MCP_DATA_DIR=~/.mcp-manager
MCP_FEEDBACK_DIR=~/.mcp-manager/feedback

# 正式版安全设置（更严格）
MCP_AUTO_APPROVE_ALL=false
MCP_REQUIRE_CONFIRMATION=true
